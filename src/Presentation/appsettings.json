{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppConfig": {
    "SpotifyAuthConfig": {
      "ClientId": "",
      "ClientSecret": "",
      "Scope": "user-modify-playback-state user-read-playback-state",
      "RedirectUri": "http://localhost:26397/callback",
      "BaseUrl": "https://accounts.spotify.com",
      "AuthenticationPath": "/api/token",
      "AuthorizePath": "/authorize"
    },
    "SpotifyConfig": {
      "DeviceWhitelist": "",
      "BaseUrl": "https://api.spotify.com/v1",
      "SearchPath": "/search",
      "DevicesPath": "/me/player/devices",
      "QueuePath": "/me/player/queue",
      "NextTrackPath": "/me/player/next",
      "PreviousTrackPath": "/me/player/previous",
      "PlayPath": "/me/player/play",
      "PausePath": "/me/player/pause",
      "StatePath": "/me/player",
      "ShufflePath": "/me/player/shuffle"
    }
  }
}
